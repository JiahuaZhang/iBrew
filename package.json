{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "generate:css": "tailwindcss --output ./app/styles/app.css",
    "build:css": "npm run generate:css -- --minify",
    "build": "npm run build:css && remix build",
    "dev:css": "npm run generate:css -- --watch",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "start": "cross-env NODE_ENV=production netlify dev",
    "typecheck": "tsc",
    "db:push": "prisma db push",
    "prisma:pull": "prisma db pull",
    "prisma:generate": "prisma generate"
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "@lexical/react": "^0.10.0",
    "@netlify/functions": "^1.3.0",
    "@preact/signals-react": "^1.2.2",
    "@prisma/client": "^4.13.0",
    "@remix-run/netlify": "^1.16.0",
    "@remix-run/node": "^1.16.0",
    "@remix-run/react": "^1.16.0",
    "@supabase/supabase-js": "^2.21.0",
    "antd": "^5.2.0",
    "cross-env": "^7.0.3",
    "lexical": "^0.10.0",
    "react": "^18.2.0",
    "react-circular-progressbar": "^2.1.0",
    "react-cmdk": "^1.3.8",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "react-social-login-buttons": "^3.6.1",
    "remix-auth": "^3.4.0",
    "remix-auth-socials": "^2.0.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.16.0",
    "@remix-run/eslint-config": "^1.16.0",
    "@remix-run/serve": "^1.16.0",
    "@types/react": "^18.0.25",
    "@types/react-circular-progressbar": "^1.1.0",
    "@types/react-dom": "^18.0.8",
    "@types/uuid": "^9.0.1",
    "concurrently": "^7.6.0",
    "eslint": "^8.27.0",
    "prisma": "^4.13.0",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=14"
  }
}
